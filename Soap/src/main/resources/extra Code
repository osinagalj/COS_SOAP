	public void setCows(List<CowOutput> cows2) {
		this.cows = cows2;
		
	}
	
    public void setAlerts(List<CowAlertOutput> alerts2) {
    	this.alerts = alerts2;
    }
    
	
    public void setAlerts(List<HerdAlertOutput> alerts2) {
    	this.alerts = alerts2;
    }
    
        public void setHerds(List<HerdOutput> herds) {
    	this.herds = herds;
    }
	
	
